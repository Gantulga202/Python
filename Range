total = 0

for i in range(11):
    if i%2==0:
        total = total + i

print(total)
print("===========================================")

for i in range(11):
    print(i)

print("===========================================")
for i in range(28):
    print(i)

print("===========================================")
for i in range(1,10,2):
    print(i)

print("===========================================")
for i in range(0,50,2):
    print(i)
